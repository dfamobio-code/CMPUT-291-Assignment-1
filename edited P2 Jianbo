CREATE table Users(
  uid int,
  name char(20) not null,
  email char(40),
  city char(20),
  time eZone char(20),
  Primary key (uid)
);

CREATE table Lists(
  name char (20),
  creator int NOT NULL,
  Primary key (creator, name),
  Foreign key (creator) Reference User(uid),
  ON DELETE CASCADE
);

CREATE table tweets(
  tdate date,
  text char(40),
  writer int NOT NULL,
  (maybe I donâ€™t)
  Primary key (tdate, writer),}
  Foreign key (writer) Reference User(uid)
  ON DELETE CASCADE
);

CREATE table URLs(
  shortURL URL,
  longURL URL,
  Primary key (shortURL),
);

CREATE table hashtags(
  term char(20),
  Primary key (term),
);

CREATE table Bot(
  uid int,
  Primary key (uid),
  Foreign key (uid) Reference Uses(uid)
);

CREATE table Blue(
  cardino chara(20),
  uid int,
  Primary key (uid),
  Foreign key (uid) Reference Users(uid)
);

CREATE table Gray(
  vdate date,
  uid int,
  Primary key (uid),
  Foreign key (uid) Reference Users(uid)
);

CREATE table follows(
  follower_id int,
  followee_id int,
  started time,
  Primary key (follower_id, followee_id),
  Foreign key (follower_id) Reference Users(uid),
  Foreign key (followee_id) Reference Users(uid)
);

CREATE table owner(
  ownid int,
  botid int,
  Primary key (ownid, botid),
  Foreign key (ownid) Reference Users(uid)
);

CREATE table affiliate(
  grayid int,
  uid int,
  Primary key (grayid, uid),
  Foreign key (uid) Reference Users(uid),
  Foreign key (grayid) Reference Gray
);

CREATE table includes(
  name char(20) not null,
  uid int,
  Primary key (Lname, uid),
  Foreign key (uid) Reference Users(uid),
  Foreign key (name) Reference Lists(name)
);

CREATE table retweets(
  rdate date,
  uid int,
  tdate date,
  Primary key (uid, tdate),
  Foreign key (tdate) Reference tweets(tdate)
  Foreign key (uid) Reference Users(uid)
);

CREATE table mentionsH(
  uid int,
  term char,
  tdate date,
  Primary key (tdate, uid, term),
  Foreign key (tdate,uid) Reference tweets(tdate,uid),
  Foreign key (term) Reference hashtags(term),
);

CREATE table replto(
uid_reply int,
	uid_original int,
tdate_reply date,
tdate_original date,
Primary key (uid_reply, uid_original, tdate_reply, tdate_original),
Foreign key (tdate_reply, uid_reply) Reference tweets(tdate, uid),
Foreign key (tdate_origianl, uid_original) Reference tweets(tdate, uid),
);

CREATE table mentionsU(
uid int,
tdate date,
shortURL URL,
Primary key (uid, tdate, shortURL)
Foreign key (uid, tdate) Reference tweets(uid, tdate),
Foreign key (shortURL) Reference URLs(shortURL),
);
